struct vIn {
  float2 tex : TEXCOORD;
  float4 pos : POSITION;
  float4 diffuse : DIFFUSE;
};

struct vOut {
  float4 pos : POSITION;
  float4 col : COLOR;
  float2 tex0 : TEXCOORD0;  // Our diffuse map
};

vOut main(vIn I) {
  vOut result;

  result.pos = I.pos;
  result.col = I.diffuse;
  result.tex0 = I.tex;

  return result;
}
